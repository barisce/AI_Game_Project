package c461a.EntityModels;

public class Range extends Unit {
	private int range;
	public Range(){
		this.setName("Range");
		this.setType("Range");
		this.setMaxHp(500);
		this.setCurrentHp(500);
		this.setKills(0);
		this.setDamage(200);
		this.setRange(3);
		this.setMoveMax(5);
		this.setMoveLeft(0);
		this.setSightRange(10);
	}
	
	public Range(int x, int y){
		this.setX(x);
		this.setY(y);
		this.setName("Range");
		this.setType("Range");
		this.setMaxHp(500);
		this.setCurrentHp(500);
		this.setKills(0);
		this.setDamage(200);
		this.setRange(3);
		this.setMoveMax(5);
		this.setMoveLeft(0);
		this.setSightRange(10);
	}
	
	public int getRange() {
		return range;
	}
	public void setRange(int range) {
		this.range = range;
	}
	
	
	
	

}
